<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>All Users</title>
</head>
<body>
    <h1>All Users</h1>

    <% if (users.length > 0) { %>
        <ul>
            <% users.forEach(user => { %>
                <li>
                    <strong>Username:</strong> <%= user.username %><br>
                    <strong>Email:</strong> <%= user.email %><br>
                    <strong>Gender:</strong> <%= user.gender %><br><br>
                </li>
            <% }) %>
        </ul>
    <% } else { %>
        <p>No users found.</p>
    <% } %>
</body>
</html>
